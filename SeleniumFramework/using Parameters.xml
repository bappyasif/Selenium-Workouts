	<!-- In Test Case Declaration Then Null Will Be Invoked Unless -->
	<!-- Optional Value Is Set In That Test Case Declaration -->
	
	<!-- <parameter name="paramEx" value="selenium"></parameter> -->
	
	<test thread-count="5" name="Test Cases">
	
		<!-- Parameters Defined Test Level -->
		<parameter name="exParam" value="automation"></parameter>
		<classes>
			<class name="testNGExamples.UsingParameters"></class>
		</classes>
		
	</test> <!-- Test Cases -->
</suite> <!-- Param Suite -->
